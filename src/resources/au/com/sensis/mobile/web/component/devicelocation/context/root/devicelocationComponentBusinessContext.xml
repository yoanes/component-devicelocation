<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:util="http://www.springframework.org/schema/util"
        xmlns:aop="http://www.springframework.org/schema/aop"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd
                http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
  			   
     <bean id="devicelocation.comp.deviceLocationDelegate"
            class="au.com.sensis.mobile.web.component.devicelocation.business.DeviceLocationDelegate">
      
        <property name="propertiesConfigLoader" ref="crf.propertiesLoader"/>
        <property name="abstractPropertiesPath" value="comp/devicelocation/devicelocation.properties" />
        <property name="supportDeviceLocationPropertyName" value="support.devicelocation" />
    </bean>
</beans>
